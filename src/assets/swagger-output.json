    "/user/create": {
      "post": {
        "tags": [
          "User"
        ],
        "description": "Endpoint\r  para criar um novo usuário.",
        "parameters": [
          {
            "name": "newUser",
            "in": "body",
            "description": "User information.",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "John Doe"
                },
                "email": {
                  "type": "string",
                  "example": "jonhdoe@email.com"
                },
                "password": {
                  "type": "string",
                  "example": "123456"
                }
              },
              "required": [
                "name",
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Usuário criado com sucesso."
          },
          "400": {
            "description": "Erro ao criar usuário."
          },
          "500": {
            "description": "Erro no servidor."
          }
        }
      }
    },
